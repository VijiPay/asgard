{
  "name": "asgard",
  "version": "2.0.0",
  "description": "vijipay Webservice",
  "main": "server.ts",
  "author": "shemang david",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node serve.js",
    "build": "tsc",
    "start:dev": "yarn build && tsx watch src/server.ts --clear-screen=false",
    "test": "vitest",
    "up:dev:nocache": "yarn build && docker-compose up --attach asgard",
    "up:dev": "yarn build && docker-compose -f docker-compose.yml -f docker-compose-cache.yml up -d && docker logs -f asgard",
    "up:dev:silent": "yarn build && docker-compose -f docker-compose.yml -f docker-compose-cache.yml up -d",
    "lint": "eslint ./src --no-cache",
    "lint:fix": "yarn lint -- --fix",
    "knip": "knip",
    "down": "docker-compose down --remove-orphans"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "@typescript-eslint/eslint-plugin": "7.7.1",
    "@typescript-eslint/parser": "7.7.1",
    "eslint": "8.56.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-unused-imports": "3.1.0",
    "knip": "5.1.3",
    "prettier": "3.2.5",
    "tsx": "4.7.2",
    "typescript": "5.4.5",
    "vitest": "1.5.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.556.0",
    "@sentry/node": "7.112.1",
    "axios": "1.6.8",
    "cors": "2.8.5",
    "express": "4.19.2",
    "http-status": "1.7.4",
    "ioredis": "5.4.1",
    "ts-node": "10.9.2",
    "tsoa": "6.2.0",
    "typeorm": "0.3.20",
    "uuid": "9.0.1"
  }
}
