{
	"jsc": {
		"parser": {
			"syntax": "typescript",
			"tsx": false,
			"decorators": true,
			"dynamicImport": true
		},
		"transform": {
			"decoratorMetadata": true
		},
		"target": "es5",
		"externalHelpers": true
	},
	"module": {
		"type": "commonjs",
		"strict": true,
		"strictMode": true
	}
}
